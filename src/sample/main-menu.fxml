<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <GridPane layoutX="-1.0" layoutY="6.0" prefHeight="394.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="adminBtn" mnemonicParsing="false" onAction="#openAdminMenu" prefHeight="52.0" prefWidth="600.0" text="Admin" GridPane.rowIndex="3" />
            <Button fx:id="versusBtn" mnemonicParsing="false" onAction="#openVersusMenu" prefHeight="49.0" prefWidth="600.0" text="1v1" />
            <Button fx:id="tournBtn" mnemonicParsing="false" onAction="#openTournMenu" prefHeight="49.0" prefWidth="600.0" text="Tournament" GridPane.rowIndex="1" />
            <Button fx:id="statsBtn" mnemonicParsing="false" onAction="#openStatsMenu" prefHeight="47.0" prefWidth="600.0" text="Statistics" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
